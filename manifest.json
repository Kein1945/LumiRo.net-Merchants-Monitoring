{
  "name": "Lumi merchant monitor",
  "version": "1.0.4",
  "description": "Merchant monitoring for ragnarok online server Lumiro.net",

  "permissions": [
    "tabs",
    "http://market.lumiro.net/*",
    "notifications"
  ],

  "browser_action": {
    "default_title": "Lumi Merchant",
    "default_icon": "Images/ico.png",
    "popup": "popup.html"
  },
  "icons": {
    "16": "Images/ico16.png",
    "48": "Images/search.png",
    "128": "Images/search.png"
  },
    "sandbox": {
        "pages": ["sandbox.html"]
    },
  "options_page": "options.html",
  "background_page": "background.html"
}